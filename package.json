{
  "name": "pleo-dashboard",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/odGit/pleo-dashboard.git"
  },
  "author": "Olga Dmitricenko",
  "license": "AGPL-3.0-only",
  "standard": {
    "ignore": [
      "dist/*",
      "pleo-code"
    ]
  },
  "scripts": {
    "clean:all": "rm -rf ./node_modules ./pleo_code/api/node_modules ./client/node_modules",
    "init": "cd ./pleo-code/api && npm i && cd ../../client && yarn && cd ../ && yarn && yarn start",
    "lint": "./node_modules/.bin/standard",
    "lint-fix": "./node_modules/.bin/standard --fix",
    "start": "concurrently --kill-others-on-fail \"npm run start:api\" \"npm run start:client\"",
    "start:api": "cd ./pleo-code/api && npm start",
    "start:client": "cd ./client && yarn start",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "concurrently": "^5.1.0",
    "standard": "^14.3.1"
  },
  "dependencies": {}
}
